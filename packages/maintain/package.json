{
  "name": "@ui8kit/maintain",
  "version": "0.1.0",
  "description": "CLI maintenance toolkit for UI8Kit projects",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "schemas",
    "bin",
    "README.md",
    "GUIDE.md"
  ],
  "bin": {
    "maintain": "./src/cli.ts"
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ajv": "^8.18.0",
    "ajv-formats": "^3.0.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "typescript": "^5.9.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "@types/node": "^25.3.1"
  }
}
